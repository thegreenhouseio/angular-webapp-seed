{
  "name": "seed-webapp",
  "version": "0.2.0",
  "author": {
    "name": "Owen Buckley",
    "email": "owen@analogstudios.net"
  },
  "description": "This is the seed repo for FrontEnd projects in The Greenhouse, based on Angular 2 and Webpack.",
  "keywords": [
    "Angular",
    "Angular 2",
    "Webpack",
    "The Greenhouse",
    "seed"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=4.4.x",
    "npm": ">=3.8.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thegreenhouseio/angular2-webpack-seed"
  },
  "scripts": {
    "build": "rimraf ./build && webpack --config ./webpack.config.prod.js -p --progress --profile --bail",
    "ci": "export NODE_ENV=production && npm run test:unit && npm run build",
    "develop": "export NODE_ENV=development && rimraf ./build && webpack-dev-server --config ./webpack.config.dev.js  --inline --progress --port 6789",
    "demo": "npm run build && ./node_modules/.bin/gulp serve",
    "install:typings": "./node_modules/.bin/typings install",
    "test:unit": "rimraf ./reports && karma start ./karma.conf.js"
  },
  "private": true,
  "readmeFilename": "README.md",
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "font-awesome": "4.6.3",
    "moment": "2.14.1",
    "reflect-metadata": "0.1.4",
    "rxjs": "5.0.0-beta.10",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "@angularclass/hmr-loader": "1.0.1",
    "angular2-template-loader": "0.4.0",
    "awesome-typescript-loader": "1.1.1",
    "babel-core": "6.14.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-webserver": "0.9.1",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "jasmine-core": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "1.8.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "systemjs": "0.19.37",
    "to-string-loader": "1.1.4",
    "typescript": "1.8.10",
    "typings": "1.3.2",
